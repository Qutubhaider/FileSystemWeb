        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta content="DMF" name="description" />
        <meta content="DMF" name="author" />
        <!-- App favicon -->
        <link rel="shortcut icon" href="https://www.jharkhand.gov.in/images/jhlogo55.PNG">

        <!-- Bootstrap Css -->
        <link href="~/assets/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
