@model dynamic
@using FileSystemBAL.Case.Models
@using FileSystemUtility.Utilities
@using FileSystemUtility.Models



@if (Model.GetCaseList != null && Model.GetCaseList.Count > 0)
{
    foreach (CaseListResult loCaseList in Model.GetCaseList)
    {
        <tr>           
            <td>@loCaseList.stDivisionName</td>
            <td>@loCaseList.stDepartmentName</td>
            <td>@loCaseList.stDesignationName</td>
            <td>@loCaseList.stFileName</td>
            <td></td>
            <td class="text-center"><a asp-area="DeskOP" asp-controller="Case" asp-action="Detail" asp-route-id="@loCaseList.unCaseId" class="btn btn-outline-secondary btn-sm edit"><i class="fas fa-pencil-alt"></i> </a></td>
        </tr>
    }
}
else
{
    <tr>
        <td colspan="8" class="text-center py-5">No records found.</td>
    </tr>
}

@{
    Pagination loPagination = Model.Pagination;

    <input type="hidden" id="hdnCurrentPage" value="@loPagination.inCurrentPage">
    <input type="hidden" id="hdnTotalPage" value="@loPagination.inTotalPages" />
    <input type="hidden" id="hdnTotalRecords" value="@loPagination.inTotalRecords" />
    <input type="hidden" id="hdnNoOfRecordsMessage" value="@loPagination.stNoOfRecordsMessage" />

}