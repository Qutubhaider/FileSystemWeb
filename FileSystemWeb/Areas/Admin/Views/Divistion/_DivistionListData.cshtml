@using FileSystemBAL.Division.Models;
@using FileSystemUtility.Utilities;
@using FileSystemUtility.Models;

@if (Model.GetDivisionList != null && Model.GetDivisionList.Count > 0)
{
    foreach (DivisionListResult loDivisionListResults in Model.GetDivisionList)
    {
        <tr>
            <td>@loDivisionListResults.stZoneName</td>
            <td>@loDivisionListResults.stDivisionName</td>
            <td class="text-center"><a asp-area="Admin" asp-controller="Division" asp-action="Detail" asp-route-id="@loDivisionListResults.unDivisionId" class="btn btn-outline-secondary btn-sm edit"><i class="fas fa-pencil-alt"></i> </a></td>
        </tr>
    }
}
else
{
    <tr>
        <td colspan="8" class="text-center py-5">No records found.</td>
    </tr>
}

@{
    Pagination loPagination = Model.Pagination;

    <input type="hidden" id="hdnCurrentPage" value="@loPagination.inCurrentPage">
    <input type="hidden" id="hdnTotalPage" value="@loPagination.inTotalPages" />
    <input type="hidden" id="hdnTotalRecords" value="@loPagination.inTotalRecords" />
    <input type="hidden" id="hdnNoOfRecordsMessage" value="@loPagination.stNoOfRecordsMessage" />

}



